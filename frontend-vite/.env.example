# Backend API URL
# Set to actual backend URL or leave empty to use mock API
VITE_API_BASE_URL=http://localhost:5004

# Enable mock API (set to 'true' to use mock data, 'false' to use real backend)
VITE_USE_MOCK_API=false

# App Environment
VITE_APP_ENV=development

# Enable debug mode
VITE_DEBUG=true
